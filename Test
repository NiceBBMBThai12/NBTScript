    end
end)
tp:Toggle("Teleport | [Tween]", nil, function(value)
    _G.RUN = value
    while _G.RUN do
local CFrameEnd = game.Players[Select].Character.HumanoidRootPart.CFrame * CFrame.new(distance_x,distance_y,distance_z)
local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(), {CFrame = CFrameEnd})
tween:Play()
tween.Completed:Wait()
end
end)
