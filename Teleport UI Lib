        NislcsUI:Destroy()
    end)

    local function EnabledDrag(dragpart, todrag)
        local UserInputService = game:GetService("UserInputService")
        
        local gui = dragpart
        local guitodrag = todrag
        
        local dragging
        local dragInput
        local dragStart
        local startPos
        
        local function update(input)
            local delta = input.Position - dragStart
            guitodrag.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
        
        gui.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                dragging = true
                dragStart = input.Position
                startPos = guitodrag.Position
        
                input.Changed:Connect(function()
                    if input.UserInputState == Enum.UserInputState.End then
                        dragging = false
                    end
                end)
            end
        end)
        
        gui.InputChanged:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
                dragInput = input
            end
        end)
        
        UserInputService.InputChanged:Connect(function(input)
            if input == dragInput and dragging then
                update(input)
            end
        end)
    end

    EnabledDrag(DragPart, ambientShadow)

    local NislcsWindow = {}

    function NislcsWindow:NewTab(tabtitle)
        local Container = Instance.new("ScrollingFrame")
        local ContainerList = Instance.new("UIListLayout")
        
        Container.Name = tabtitle or "Container"
        Container.Parent = ContainerHolder
        Container.Active = true
        Container.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
        Container.BorderSizePixel = 0
        Container.Position = UDim2.new(0, 5, 0, 5)
        Container.Size = UDim2.new(0, 0, 0, 260)
        Container.ZIndex = 3
        Container.CanvasSize = UDim2.new(0, 0, 0, 0)
        Container.ScrollBarThickness = 0
        
        ContainerList.Name = "ContainerList"
        ContainerList.Parent = Container
        ContainerList.HorizontalAlignment = Enum.HorizontalAlignment.Center
        ContainerList.SortOrder = Enum.SortOrder.LayoutOrder
        ContainerList.Padding = UDim.new(0, 5)

        local function ResizeContainer()
            Container.CanvasSize = UDim2.new(0, 0, 0, ContainerList.AbsoluteContentSize.Y + ContainerList.Padding.Offset)
        end
        ResizeContainer()
        ContainerList:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(ResizeContainer)

        local TabButton = Instance.new("TextButton")
        local TabButtonUICorner = Instance.new("UICorner")
        
        TabButton.Name = tabtitle or "TabButton"
        TabButton.Parent = TabHolder
        TabButton.BackgroundColor3 = Color3.fromRGB(255,95,0)
        TabButton.BackgroundTransparency = 1
        TabButton.BorderSizePixel = 0
        TabButton.Position = UDim2.new(0, 4, 0, 4)
        TabButton.Size = UDim2.new(0, 165, 0, 25)
        TabButton.ZIndex = 4
