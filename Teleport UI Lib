            end)
            local toggled = false
            ToggleButton.MouseButton1Click:Connect(function()
                if toggled then
                    toggled = false
                    game:GetService("TweenService"):Create(TogglePosition, TweenInfo.new(0.32, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Position = UDim2.new(0, 4, 0, 2)}):Play()
                    game:GetService("TweenService"):Create(Togglecheck, TweenInfo.new(0.32, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 1}):Play()
                    game:GetService("TweenService"):Create(Togglecheck, TweenInfo.new(0.32, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 1}):Play()
                    game:GetService("TweenService"):Create(Toggleuncheck, TweenInfo.new(0.16, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 0}):Play()
                    game:GetService("TweenService"):Create(Toggleuncheck, TweenInfo.new(0.16, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 0}):Play()
                    pcall(function()
                        togglecallback(toggled)
                    end)
                else
                    toggled = true
                    game:GetService("TweenService"):Create(TogglePosition, TweenInfo.new(0.32, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Position = UDim2.new(0, 21, 0, 2)}):Play()
                    game:GetService("TweenService"):Create(Toggleuncheck, TweenInfo.new(0.32, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 1}):Play()
                    game:GetService("TweenService"):Create(Toggleuncheck, TweenInfo.new(0.32, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 1}):Play()
                    game:GetService("TweenService"):Create(Togglecheck, TweenInfo.new(0.16, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 0}):Play()
                    game:GetService("TweenService"):Create(Togglecheck, TweenInfo.new(0.16, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 0}):Play()
                    pcall(function()
                        togglecallback(toggled)
                    end)
                end
            end)

        end

        function NislcsTab:NewDropdown(dropdowntitle, list, dropdowncallback)
            list = list or {}

            local DropdownHolder = Instance.new("ImageLabel")
            local DropdownHolderUICorner = Instance.new("UICorner")
            local DropdownButton = Instance.new("TextButton")
            local DropdownButtonUICorner = Instance.new("UICorner")
            local DropdownUnPlus = Instance.new("ImageLabel")
            local DropdownUnPlusUICorner = Instance.new("UICorner")
            local DropdownPlus = Instance.new("ImageLabel")
            local DropdownPlusUICorner = Instance.new("UICorner")
            local DropdownHolderTitle = Instance.new("TextLabel")
            local DropdownIn = Instance.new("Frame")
            local DropdownInList = Instance.new("UIListLayout")
            
            --Properties:
            
            DropdownHolder.Name = dropdowntitle or "DropdownHolder"
            DropdownHolder.Parent = Container
            DropdownHolder.AnchorPoint = Vector2.new(0.5, 0.5)
            DropdownHolder.BackgroundColor3 = Color3.fromRGB(68, 88, 200)
            DropdownHolder.BackgroundTransparency = 1.000
            DropdownHolder.BorderSizePixel = 0
            DropdownHolder.ClipsDescendants = true
            DropdownHolder.Position = UDim2.new(0, 70, 0, 12)
            DropdownHolder.Size = UDim2.new(0, 364, 0, 30)
            DropdownHolder.ZIndex = 4
            DropdownHolder.Image = "rbxassetid://1316045217"
            DropdownHolder.ImageTransparency = 1.000
            DropdownHolder.ScaleType = Enum.ScaleType.Slice
            DropdownHolder.SliceCenter = Rect.new(10, 10, 118, 118)
            
            DropdownHolderUICorner.CornerRadius = UDim.new(0, 4)
            DropdownHolderUICorner.Name = "DropdownHolderUICorner"
            DropdownHolderUICorner.Parent = DropdownHolder
            
            DropdownButton.Name = "DropdownButton"
            DropdownButton.Parent = DropdownHolder
            DropdownButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
            DropdownButton.BorderSizePixel = 0
            DropdownButton.Position = UDim2.new(0, 3, 0, 3)
            DropdownButton.Size = UDim2.new(0, 358, 0, 22)
            DropdownButton.ZIndex = 5
            DropdownButton.AutoButtonColor = false
            DropdownButton.Font = Enum.Font.Gotham
            DropdownButton.Text = ""
            DropdownButton.TextColor3 = Color3.fromRGB(160, 160, 160)
            DropdownButton.TextSize = 13.000
            
            DropdownButtonUICorner.CornerRadius = UDim.new(0, 4)
            DropdownButtonUICorner.Name = "DropdownButtonUICorner"
            DropdownButtonUICorner.Parent = DropdownButton
            
            DropdownUnPlus.Name = "DropdownUnPlus"
            DropdownUnPlus.Parent = DropdownButton
            DropdownUnPlus.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
            DropdownUnPlus.BackgroundTransparency = 1.000
            DropdownUnPlus.BorderSizePixel = 0
            DropdownUnPlus.Position = UDim2.new(0, 335, 0, 1)
            DropdownUnPlus.Size = UDim2.new(0, 20, 0, 20)
            DropdownUnPlus.ZIndex = 7
            DropdownUnPlus.Image = "rbxassetid://7954947632"
            DropdownUnPlus.ImageColor3 = Color3.fromRGB(220, 220, 220)
            DropdownUnPlus.ImageTransparency = 1.000
            
            DropdownUnPlusUICorner.CornerRadius = UDim.new(0, 4)
            DropdownUnPlusUICorner.Name = "DropdownUnPlusUICorner"
            DropdownUnPlusUICorner.Parent = DropdownUnPlus
            
            DropdownPlus.Name = "DropdownPlus"
            DropdownPlus.Parent = DropdownUnPlus
            DropdownPlus.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
            DropdownPlus.BorderSizePixel = 0
            DropdownPlus.Size = UDim2.new(0, 20, 0, 20)
            DropdownPlus.ZIndex = 7
            DropdownPlus.Image = "rbxassetid://7955129634"
            DropdownPlus.ImageColor3 = Color3.fromRGB(220, 220, 220)
            
            DropdownPlusUICorner.CornerRadius = UDim.new(0, 4)
            DropdownPlusUICorner.Name = "DropdownPlusUICorner"
            DropdownPlusUICorner.Parent = DropdownPlus
            
            DropdownHolderTitle.Name = "DropdownHolderTitle"
            DropdownHolderTitle.Parent = DropdownHolder
            DropdownHolderTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownHolderTitle.BackgroundTransparency = 1.000
            DropdownHolderTitle.BorderSizePixel = 0
            DropdownHolderTitle.Position = UDim2.new(0, 10, 0, 0)
            DropdownHolderTitle.Size = UDim2.new(0, 195, 0, 28)
            DropdownHolderTitle.ZIndex = 6
            DropdownHolderTitle.Font = Enum.Font.Gotham
            DropdownHolderTitle.Text = dropdowntitle or "Dropdown Example"
            DropdownHolderTitle.TextColor3 = Color3.fromRGB(160, 160, 160)
            DropdownHolderTitle.TextSize = 13.000
            DropdownHolderTitle.TextXAlignment = Enum.TextXAlignment.Left
            
            DropdownIn.Name = "DropdownIn"
            DropdownIn.Parent = DropdownHolder
            DropdownIn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownIn.BorderSizePixel = 0
            DropdownIn.Position = UDim2.new(0, 0, 0, 30)
            DropdownIn.Size = UDim2.new(0, 364, 0, 0)
            DropdownIn.ZIndex = 10
            
            DropdownInList.Name = "DropdownInList"
            DropdownInList.Parent = DropdownIn
            DropdownInList.HorizontalAlignment = Enum.HorizontalAlignment.Center
            DropdownInList.SortOrder = Enum.SortOrder.LayoutOrder
            DropdownInList.Padding = UDim.new(0, 5)

            DropdownButton.MouseEnter:Connect(function()
                game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.12, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {BackgroundColor3 = Color3.fromRGB(35, 35, 35)}):Play()
            end)
            DropdownButton.MouseLeave:Connect(function()
                game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.12, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
            end)
            DropdownButton.MouseButton1Click:Connect(function()
                game:GetService("TweenService"):Create(DropdownHolder, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Size = UDim2.new(0, 364, 0, 30 + DropdownInList.AbsoluteContentSize.Y + DropdownInList.Padding.Offset)}):Play()
                game:GetService("TweenService"):Create(DropdownUnPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 0}):Play()
                game:GetService("TweenService"):Create(DropdownUnPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 0}):Play()
                game:GetService("TweenService"):Create(DropdownPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 1}):Play()
                game:GetService("TweenService"):Create(DropdownPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 1}):Play()
            end)

            for listindex, listvalue in next, list do
                local DropdownList = Instance.new("TextButton")
                local DropdownListUICorner = Instance.new("UICorner")
                
                DropdownList.Name = listvalue or "DropdownList"
                DropdownList.Parent = DropdownIn
                DropdownList.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
                DropdownList.BorderSizePixel = 0
                DropdownList.Size = UDim2.new(0, 358, 0, 18)
                DropdownList.ZIndex = 11
                DropdownList.AutoButtonColor = false
                DropdownList.Font = Enum.Font.Gotham
                DropdownList.Text = listvalue or "List Example"
                DropdownList.TextColor3 = Color3.fromRGB(150, 150, 150)
                DropdownList.TextSize = 13.000
                
                DropdownListUICorner.CornerRadius = UDim.new(0, 4)
                DropdownListUICorner.Name = "DropdownListUICorner"
                DropdownListUICorner.Parent = DropdownList

                DropdownList.MouseEnter:Connect(function()
                    game:GetService("TweenService"):Create(DropdownList, TweenInfo.new(0.12, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {BackgroundColor3 = Color3.fromRGB(35, 35, 35)}):Play()
                end)
                DropdownList.MouseLeave:Connect(function()
                    game:GetService("TweenService"):Create(DropdownList, TweenInfo.new(0.12, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
                end)
                DropdownList.MouseButton1Click:Connect(function()
                    pcall(function()
                        dropdowncallback(DropdownList.Name)
                    end)
                    game:GetService("TweenService"):Create(DropdownHolder, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Size = UDim2.new(0, 364, 0, 30)}):Play()
                    game:GetService("TweenService"):Create(DropdownUnPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 1}):Play()
                    game:GetService("TweenService"):Create(DropdownUnPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 1}):Play()
                    game:GetService("TweenService"):Create(DropdownPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 0}):Play()
                    game:GetService("TweenService"):Create(DropdownPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 0}):Play()
                end)

            end

            local NislcsDropdown = {}

            function NislcsDropdown:RefreshList(newlist)
                newlist = newlist or {}
                for _, alllist in pairs(DropdownIn:GetChildren()) do
                    if alllist:IsA("TextButton") then
                        alllist:Destroy()
                    end
                end
                for indexlistn, valuelistn in next, newlist do
                    local DropdownList = Instance.new("TextButton")
                    local DropdownListUICorner = Instance.new("UICorner")
                    
                    DropdownList.Name = valuelistn or "DropdownList"
                    DropdownList.Parent = DropdownIn
                    DropdownList.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
                    DropdownList.BorderSizePixel = 0
                    DropdownList.Size = UDim2.new(0, 358, 0, 18)
                    DropdownList.ZIndex = 11
                    DropdownList.AutoButtonColor = false
                    DropdownList.Font = Enum.Font.Gotham
                    DropdownList.Text = valuelistn or "List Example"
                    DropdownList.TextColor3 = Color3.fromRGB(150, 150, 150)
                    DropdownList.TextSize = 13.000
                    
                    DropdownListUICorner.CornerRadius = UDim.new(0, 4)
                    DropdownListUICorner.Name = "DropdownListUICorner"
                    DropdownListUICorner.Parent = DropdownList
    
                    DropdownList.MouseEnter:Connect(function()
                        game:GetService("TweenService"):Create(DropdownList, TweenInfo.new(0.12, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {BackgroundColor3 = Color3.fromRGB(35, 35, 35)}):Play()
                    end)
                    DropdownList.MouseLeave:Connect(function()
                        game:GetService("TweenService"):Create(DropdownList, TweenInfo.new(0.12, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
                    end)
                    DropdownList.MouseButton1Click:Connect(function()
                        pcall(function()
                            dropdowncallback(DropdownList.Name)
                        end)
                        game:GetService("TweenService"):Create(DropdownHolder, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Size = UDim2.new(0, 364, 0, 30)}):Play()
                        game:GetService("TweenService"):Create(DropdownUnPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 1}):Play()
                        game:GetService("TweenService"):Create(DropdownUnPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 1}):Play()
                        game:GetService("TweenService"):Create(DropdownPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 0}):Play()
                        game:GetService("TweenService"):Create(DropdownPlus, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {ImageTransparency = 0}):Play()
                    end)
    
                end
            end
            
            return NislcsDropdown
        end

        function NislcsTab:NewTextBox(textboxtitle, textboxcallback)
            local TextBoxHolder = Instance.new("ImageLabel")
            local TextBoxHolderUICorner = Instance.new("UICorner")
            local TextBoxHolderFrame = Instance.new("Frame")
            local TextBoxHolderTitle = Instance.new("TextLabel")
            local TextBox = Instance.new("TextBox")
            local TextBoxUICorner = Instance.new("UICorner")
            
            TextBoxHolder.Name = textboxtitle or "TextBoxHolder"
            TextBoxHolder.Parent = Container
            TextBoxHolder.AnchorPoint = Vector2.new(0.5, 0.5)
            TextBoxHolder.BackgroundColor3 = Color3.fromRGB(68, 88, 200)
            TextBoxHolder.BackgroundTransparency = 1.000
            TextBoxHolder.BorderSizePixel = 0
            TextBoxHolder.Position = UDim2.new(0, 70, 0, 12)
            TextBoxHolder.Size = UDim2.new(0, 364, 0, 28)
            TextBoxHolder.ZIndex = 4
            TextBoxHolder.Image = "rbxassetid://1316045217"
            TextBoxHolder.ImageTransparency = 0.700
            TextBoxHolder.ScaleType = Enum.ScaleType.Slice
            TextBoxHolder.SliceCenter = Rect.new(10, 10, 118, 118)
            
            TextBoxHolderUICorner.CornerRadius = UDim.new(0, 4)
            TextBoxHolderUICorner.Name = "TextBoxHolderUICorner"
            TextBoxHolderUICorner.Parent = TextBoxHolder
            
            TextBoxHolderFrame.Name = "TextBoxHolderFrame"
            TextBoxHolderFrame.Parent = TextBoxHolder
            TextBoxHolderFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
            TextBoxHolderFrame.BorderSizePixel = 0
            TextBoxHolderFrame.Position = UDim2.new(0, 3, 0, 3)
            TextBoxHolderFrame.Size = UDim2.new(0, 358, 0, 22)
            TextBoxHolderFrame.ZIndex = 5
            
            TextBoxHolderTitle.Name = "TextBoxHolderTitle"
            TextBoxHolderTitle.Parent = TextBoxHolderFrame
            TextBoxHolderTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxHolderTitle.BackgroundTransparency = 1.000
            TextBoxHolderTitle.BorderSizePixel = 0
            TextBoxHolderTitle.Position = UDim2.new(0, 7, 0, -3)
            TextBoxHolderTitle.Size = UDim2.new(0, 198, 0, 28)
            TextBoxHolderTitle.ZIndex = 6
            TextBoxHolderTitle.Font = Enum.Font.Gotham
            TextBoxHolderTitle.Text = textboxtitle or "TextBox Example"
            TextBoxHolderTitle.TextColor3 = Color3.fromRGB(160, 160, 160)
            TextBoxHolderTitle.TextSize = 13.000
            TextBoxHolderTitle.TextXAlignment = Enum.TextXAlignment.Left
            
            TextBox.Parent = TextBoxHolderFrame
            TextBox.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
            TextBox.BorderSizePixel = 0
            TextBox.Position = UDim2.new(0, 356, 0, 1)
            TextBox.Size = UDim2.new(0, -45, 0, 20)
            TextBox.ZIndex = 10
            TextBox.ClearTextOnFocus = false
            TextBox.Font = Enum.Font.Gotham
            TextBox.Text = ""
            TextBox.TextColor3 = Color3.fromRGB(150, 150, 150)
            TextBox.TextSize = 13.000
            
            TextBoxUICorner.CornerRadius = UDim.new(0, 4)
            TextBoxUICorner.Name = "TextBoxUICorner"
            TextBoxUICorner.Parent = TextBox

            local function ResizeTextBox()
                if TextBox.Text == "" or nil then
                    TextBox:TweenSize(UDim2.new(0, -45, 0, 20), "InOut", "Quad", 0.12, true)
                else
                    TextBox:TweenSize(UDim2.new(0, -10 - TextBox.TextBounds.X, 0, 20), "InOut", "Quad", 0.12, true)
                end
            end
            TextBox:GetPropertyChangedSignal("Text"):Connect(ResizeTextBox, ResizeNoTextTextBox)
            TextBox.FocusLost:Connect(function()
                pcall(function()
                    textboxcallback(TextBox.Text)
                end)
            end)

        end

        return NislcsTab
    end

    return NislcsWindow
end

return Nislcs
