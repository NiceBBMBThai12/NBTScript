        TabButton.Text = tabtitle or "None Page"
        TabButton.TextColor3 = Color3.fromRGB(180, 180, 180)
        TabButton.TextSize = 14.000
        
        TabButtonUICorner.CornerRadius = UDim.new(0, 4)
        TabButtonUICorner.Name = "TabButtonUICorner"
        TabButtonUICorner.Parent = TabButton

        TabButton.MouseEnter:Connect(function()
            game:GetService("TweenService"):Create(TabButton, TweenInfo.new(0.12, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {TextColor3 = Color3.fromRGB(150, 150, 150)}):Play()
        end)
        TabButton.MouseLeave:Connect(function()
            game:GetService("TweenService"):Create(TabButton, TweenInfo.new(0.12, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {TextColor3 = Color3.fromRGB(180, 180, 180)}):Play()
        end)
        TabButton.MouseButton1Click:Connect(function()
            for _, tab in pairs(TabHolder:GetChildren()) do
                if tab:IsA("TextButton") then
                    game:GetService("TweenService"):Create(tab, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 1}):Play()
                end
            end
            for _, cont in pairs(ContainerHolder:GetChildren()) do
                if cont:IsA("ScrollingFrame") then
                    game:GetService("TweenService"):Create(cont, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Size = UDim2.new(0, 0, 0, 260)}):Play()
                end
            end
            game:GetService("TweenService"):Create(TabButton, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {BackgroundTransparency = 0.4}):Play()
            wait(0.12)
            game:GetService("TweenService"):Create(Container, TweenInfo.new(0.12, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {Size = UDim2.new(0, 370, 0, 260)}):Play()
        end)

